<?xml version="1.0" encoding="utf-8"?>
<ControlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<StyleNormal>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;231;54;54</Color1>
			<Color2>ARGBColor;255;147;34;34</Color2>
			<Color3>ARGBColor;255;102;31;31</Color3>
			<Color4>ARGBColor;255;189;44;44</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.4</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;97;02;10</BorderColor>
		<TextColor>ARGBColor;255;215;255;227</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleNormal>
	<StyleHighlight>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;241;11;23</Color1>
			<Color2>ARGBColor;255;147;34;34</Color2>
			<Color3>ARGBColor;255;82;11;11</Color3>
			<Color4>ARGBColor;255;179;34;34</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.4</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;97;02;10</BorderColor>
		<TextColor>ARGBColor;255;255;100;100</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleHighlight>
	<StylePressed>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;241;11;23</Color1>
			<Color2>ARGBColor;255;97;2;10</Color2>
			<LinearGradientMode>Vertical</LinearGradientMode>
		</FillStyle>
		<BorderColor>ARGBColor;255;153;21;32</BorderColor>
		<TextColor>ARGBColor;255;246;90;90</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StylePressed>
	<StyleDisabled>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;229;229;229</Color1>
			<Color2>ARGBColor;255;211;211;211</Color2>
			<Color3>ARGBColor;255;200;200;200</Color3>
			<Color4>ARGBColor;255;235;235;235</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.4</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;141;141;141</BorderColor>
		<TextColor>ARGBColor;255;141;141;141</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleDisabled>

	<StyleDisabledPressed>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;209;209;209</Color1>
			<Color2>ARGBColor;255;191;191;191</Color2>
			<Color3>ARGBColor;255;180;180;180</Color3>
			<Color4>ARGBColor;255;215;215;215</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.3</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;121;121;121</BorderColor>
		<TextColor>ARGBColor;255;121;121;121</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleDisabledPressed>

	<Radius>1</Radius>
	<ShapeType>RoundedRectangle</ShapeType>
	<Opacity>0</Opacity>
	<Animated>true</Animated>

	<!-- Color Setters -->
	<ColorSetter>
		<Name>ANamedColor</Name>
		<Color>ARGBColor;255;0;0;0</Color>
	</ColorSetter>

	<!-- FillStyle Setters -->
	<FillStyleSetter>
		<Name>AFillStyle</Name>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;255;242;191</Color1>
			<Color2>ARGBColor;255;255;181;130</Color2>
		</FillStyle>
	</FillStyleSetter>

	<!-- NamedValue Setters -->
	<NamedValueSetter>
		<Name>ScrollBarShowButtonBorder</Name>
		<Value>false</Value>
	</NamedValueSetter>

</ControlTheme>
