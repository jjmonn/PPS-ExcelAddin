<?xml version="1.0" encoding="utf-8"?>
<ControlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<StyleNormal>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;255;255;255</Color1>
			<Color2>ARGBColor;255;192;225;205</Color2>
			<LinearGradientMode>Vertical</LinearGradientMode>
		</FillStyle>
		<BorderColor>ARGBColor;255;121;155;133</BorderColor>
		<TextColor>ARGBColor;255;0;0;0</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleNormal>
	<StyleHighlight>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;255;255;255</Color1>
			<Color2>ARGBColor;255;225;197;207</Color2>
			<LinearGradientMode>Vertical</LinearGradientMode>
		</FillStyle>
		<BorderColor>ARGBColor;255;151;125;133</BorderColor>
		<TextColor>ARGBColor;255;0;0;0</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleHighlight>
	<StylePressed>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;255;255;255</Color1>
			<Color2>ARGBColor;255;241;193;95</Color2>
			<LinearGradientMode>Vertical</LinearGradientMode>
		</FillStyle>
		<BorderColor>ARGBColor;255;252;159;64</BorderColor>
		<TextColor>ARGBColor;255;0;0;0</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StylePressed>
	<StyleDisabled>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;229;229;229</Color1>
			<Color2>ARGBColor;255;211;211;211</Color2>
			<Color3>ARGBColor;255;200;200;200</Color3>
			<Color4>ARGBColor;255;235;235;235</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.4</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;141;141;141</BorderColor>
		<TextColor>ARGBColor;255;141;141;141</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleDisabled>

	<StyleDisabledPressed>
		<FillStyle xsi:type="FillStyleGradientEx">
			<Color1>ARGBColor;255;209;209;209</Color1>
			<Color2>ARGBColor;255;191;191;191</Color2>
			<Color3>ARGBColor;255;180;180;180</Color3>
			<Color4>ARGBColor;255;215;215;215</Color4>
			<GradientAngle>90</GradientAngle>
			<GradientOffset>0.3</GradientOffset>
			<GradientOffset2>0.5</GradientOffset2>
		</FillStyle>
		<BorderColor>ARGBColor;255;121;121;121</BorderColor>
		<TextColor>ARGBColor;255;121;121;121</TextColor>
		<TextAlignment>MiddleCenter</TextAlignment>
		<TextWrap>true</TextWrap>
		<Font>
			<FontFamily>Microsoft Sans Serif</FontFamily>
			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleDisabledPressed>

	<Radius>1</Radius>
	<ShapeType>RoundedRectangle</ShapeType>
	<Opacity>0</Opacity>
	<Animated>true</Animated>

	<!-- Color Setters -->
	<ColorSetter>
		<Name>EmptySetter</Name>
		<Color>ARGBColor;255;33;33;33</Color>
	</ColorSetter>

  <ColorSetter>
    <Name>MenuBorder</Name>
    <Color>ARGBColor;255;0;200;0</Color>
  </ColorSetter>

  <!--Ribbon Settings-->
  <FillStyleSetter>
    <Name>RibbonTitle</Name>
    <FillStyle xsi:type="FillStyleGradientEx">
      <Color1>ARGBColor;255;192;225;205</Color1>
      <Color2>ARGBColor;255;192;225;205</Color2>
      <Color3>ARGBColor;255;192;225;205</Color3>
      <Color4>ARGBColor;255;192;225;205</Color4>
      <GradientAngle>90</GradientAngle>
      <GradientOffset>0.2</GradientOffset>
      <GradientOffset2>0.6</GradientOffset2>
    </FillStyle>
  </FillStyleSetter>
  <ColorSetter>
    <Name>RibbonFormBorder</Name>
    <Color>ARGBColor;255;58;90;130</Color>
  </ColorSetter>
  <!-- End of ribbon settings -->
	<!-- FillStyle Setters -->

  <FillStyleSetter>
    <Name>MenuImageArea</Name>
    <FillStyle xsi:type="FillStyleGradientEx">
      <Color1>ARGBColor;255;255;255;255</Color1>
      <Color2>ARGBColor;255;255;255;255</Color2>
      <Color3>ARGBColor;255;192;225;205</Color3>
      <Color4>ARGBColor;255;192;225;205</Color4>
      <GradientAngle>90</GradientAngle>
      <GradientOffset>0.4</GradientOffset>
      <GradientOffset2>1.0</GradientOffset2>
    </FillStyle>
  </FillStyleSetter>
  <FillStyleSetter>
    <Name>MenuPressed</Name>
    <FillStyle xsi:type="FillStyleGradientEx">
      <Color1>ARGBColor;255;255;255;255</Color1>
      <Color2>ARGBColor;255;255;255;255</Color2>
      <Color3>ARGBColor;255;225;197;207</Color3>
      <Color4>ARGBColor;255;225;197;207</Color4>
      <GradientAngle>90</GradientAngle>
      <GradientOffset>0.4</GradientOffset>
      <GradientOffset2>1.0</GradientOffset2>
    </FillStyle>
  </FillStyleSetter>
  <FillStyleSetter>
    <Name>MenuHighlight</Name>
    <FillStyle xsi:type="FillStyleGradientEx">
      <Color1>ARGBColor;255;255;255;255</Color1>
      <Color2>ARGBColor;255;255;255;255</Color2>
      <Color3>ARGBColor;255;225;197;207</Color3>
      <Color4>ARGBColor;255;225;197;207</Color4>
      <GradientAngle>90</GradientAngle>
      <GradientOffset>0.4</GradientOffset>
      <GradientOffset2>1.0</GradientOffset2>
    </FillStyle>
  </FillStyleSetter>
  <FillStyleSetter>
    <Name>MenuNormal</Name>
    <FillStyle xsi:type="FillStyleGradientEx">
      <Color1>ARGBColor;255;255;255;255</Color1>
      <Color2>ARGBColor;255;255;255;255</Color2>
      <Color3>ARGBColor;255;192;225;205</Color3>
      <Color4>ARGBColor;255;192;225;205</Color4>
      <GradientAngle>90</GradientAngle>
      <GradientOffset>0.4</GradientOffset>
      <GradientOffset2>1.0</GradientOffset2>
    </FillStyle>
  </FillStyleSetter>
  
	<FillStyleSetter>
		<Name>ScrollElementNormal</Name>
		<FillStyle xsi:type="FillStyleSolid">
			<Color>ARGBColor;255;255;255;255</Color>
		</FillStyle>
	</FillStyleSetter>

	<FillStyleSetter>
		<Name>ProgressFill</Name>
		<FillStyle xsi:type="FillStyleGradient">
			<Color1>ARGBColor;255;255;223;125</Color1>
			<Color2>ARGBColor;255;241;193;95</Color2>
			<LinearGradientMode>Vertical</LinearGradientMode>
		</FillStyle>
	</FillStyleSetter>

	<!-- NamedValue Setters -->
	<NamedValueSetter>
		<Name>ScrollBarShowButtonBorder</Name>
		<Value>true</Value>
	</NamedValueSetter>

	<NamedValueSetter>
		<Name>ScrollBarThumbCornersRadius</Name>
		<Value>1</Value>
	</NamedValueSetter>

</ControlTheme>
