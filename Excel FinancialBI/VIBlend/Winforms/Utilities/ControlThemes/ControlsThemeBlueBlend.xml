	<!-- NamedValue Setters -->
	<NamedValueSetter>
		<Name>ScrollBarShowButtonBorder</Name>
		<Value>true</Value>
	</NamedValueSetter>

	<NamedValueSetter>
		<Name>ScrollBarThumbCornersRadius</Name>
