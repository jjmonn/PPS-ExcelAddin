			<GraphicsUnit>Point</GraphicsUnit>
			<Size>8.25</Size>
			<Style>Regular</Style>
		</Font>
	</StyleDisabledPressed>

	<Radius>1</Radius>
	<ShapeType>RoundedRectangle</ShapeType>
	<Opacity>0</Opacity>
	<Animated>true</Animated>

	<!-- Color Setters -->
	<ColorSetter>
		<Name>ScrollBarBorderNormal</Name>
		<Color>ARGBColor;255;130;150;180</Color>
	</ColorSetter>
	<ColorSetter>
		<Name>ScrollBorderNormal</Name>
