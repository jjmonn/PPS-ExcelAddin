	</ColorSetter>
	<ColorSetter>
		<Name>ScrollBorderHighlight</Name>
		<Color>ARGBColor;255;110;130;160</Color>
	</ColorSetter>
	<ColorSetter>
		<Name>ScrollBorderPressed</Name>
		<Color>ARGBColor;255;90;110;140</Color>
	</ColorSetter>

